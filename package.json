{
  "name": "test-site",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev-sync-git": "git push origin develop-FE-cloudfront:develop-FE-cloudfront",
    "dev-sync-s3": "aws s3 sync dist/ s3://app.samplead.co",
    "dev-invalidate": "aws cloudfront create-invalidation --distribution-id E1NINDZ6QKQ5SP --paths /*",
    "dev-deploy": "npm run build && npm run dev-sync-s3 && npm run dev-invalidate",
    "prod-sync-s3": "aws s3 sync dist/ s3://app.samplead.co",
    "prod-invalidate": "aws cloudfront create-invalidation --distribution-id E1NINDZ6QKQ5SP --paths /*",
    "prod-deploy": "npm run build && npm run prod-sync-s3 && npm run prod-invalidate",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}